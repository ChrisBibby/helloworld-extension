{
  "name": "hello-github-world",
  "displayName": "Github Action Demo Extension",
  "description": "Extension to demonstrate github action publish",
  "publisher": "chrisbibby",
  "author": {
    "name": "Chris Bibby"
  },
  "version": "1.0.5",
  "engines": {
    "vscode": "^1.75.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisBibby/helloworld-extension.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hello-github-world.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "helloworld.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "npx @vscode/vsce package"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "typescript": "^4.9.4"
  }
}
